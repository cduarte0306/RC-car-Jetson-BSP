/dts-v1/;
/plugin/;

/ {
    compatible = "nvidia,jetson-nano";

    fragment@0 {
        target = <&spi1>;
        __overlay__ {
            status = "okay";

            spidev@0 {
                compatible = "spidev";
                reg = <0>;
                spi-max-frequency = <25000000>;
            };
        };
    };
};
